{
  "name": "sdr-backend",
  "version": "1.1.1",
  "description": "Sentier des Reves backend app",
  "main": "index.js",
  "keywords": [],
  "author": "Sorok-dva",
  "license": "ISC",
  "scripts": {
    "start": "DEBUG=sdr-api:* ts-node src/server.ts",
    "build": "run-p build:*",
    "build:all": "tsc -d",
    "dev": "DEBUG=sdr-api:* CHOKIDAR_USEPOLLING=true nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "describe": "npm-scripts-info",
    "release": "npx standard-version -s",
    "test": "run-s test:**",
    "test:ts": "tsc --noEmit",
    "test:lint": "eslint 'src/**/*.ts' --fix",
    "!test:unit": "mocha 'src/**/*.test.ts'",
    "!test:db": "mocha -r ts-node/register 'test/db/*.test.ts'",
    "!test:security": "mocha -r ts-node/register 'test/security*/**/*.test.ts'",
    "test:integration": "mocha -r ts-node/register 'test/integration*/**/*.test.ts'",
    "coverage": "run-s coverage:*",
    "coverage:unit": "nyc --silent npm run test:unit",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "coverage:codecov": "codecov",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org sentier-des-reves --project sdr-backend . && sentry-cli sourcemaps upload --org sentier-des-reves --project sdr-backend .",
    "vulnerabilities": "snyk monitor"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint"
    ]
  },
  "dependencies": {
    "@sentry/cli": "^2.33.1",
    "@sentry/node": "^8.24.0",
    "@sentry/profiling-node": "^8.24.0",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "debug": "^4.3.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.0",
    "node-mailjet": "^6.0.5",
    "nodemailer": "^6.9.14",
    "sequelize": "^6.37.3",
    "util": "^0.12.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@faker-js/faker": "7.6.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/archiver": "^6.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "4.3.3",
    "@types/chai-as-promised": "7.1.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "10.0.7",
    "@types/multer": "^1.4.11",
    "@types/node": "20.14.8",
    "@types/node-mailjet": "^3.3.12",
    "@types/nodemailer": "^6.4.15",
    "@types/proxyquire": "^1.3.31",
    "@types/sequelize": "4.28.20",
    "@types/sinon": "10.0.13",
    "@types/sinon-chai": "3.2.8",
    "@types/sinon-stub-promise": "2.1.10",
    "@types/supertest": "6.0.2",
    "@types/ws": "8.5.11",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "chai-exclude": "2.1.0",
    "chai-http": "4.3.0",
    "chai-match": "1.1.1",
    "eslint": "^8.57.0",
    "eslint-config-materya": "10.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-mocha": "10.4.3",
    "if": "^2.0.0",
    "mocha": "10.6.0",
    "mocked-env": "1.3.5",
    "nock": "13.5.4",
    "nodemon": "3.1.4",
    "npm-run-all": "4.1.5",
    "proxyquire": "^2.1.3",
    "sequelize-cli": "^6.6.2",
    "sinon": "18.0.0",
    "sinon-chai": "3.7.0",
    "sinon-stub-promise": "4.0.0",
    "supertest": "7.0.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.5.3",
    "typescript-eslint": "7.16.0"
  }
}
